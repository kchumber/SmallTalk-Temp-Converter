BorderedMorph subclass: #TemperatureMorph	instanceVariableNames: 'tLabel tButton tText value convertedValue'	classVariableNames: ''	poolDictionaries: ''	category: 'SA-Assignment'!!TemperatureMorph methodsFor: 'as yet unclassified' stamp: 'KallumChumber 3/31/2017 10:39'!initialize	super initialize.	self bounds: (0@0 corner:300@200).	color := Color defaultBackgroundColor.	tLabel := StringMorph contents: 'Tempurature Converter'.	tLabel position:100@70.	tLabel color: Color black.	self addMorph: tLabel.	tButton := SimpleButtonMorph new.	tButton label:'Convert Temperature'.	tButton color: Color red.	tButton bounds:(70@170 corner:220@140).	tButton target: self.	tButton actionSelector: #buttonPressed.	self addMorph: tButton.	tText := TextMorph new.	tText contents: 'Number' .	tText bounds: (100@100 corner: 150@150) .	tText color: Color white .	self addMorph: tText.! !!TemperatureMorph methodsFor: 'as yet unclassified' stamp: 'KallumChumber 3/31/2017 10:01'!buttonPressed	value := tText contents asNumber .	convertedValue := ((value - 32.0) * 5.0 /9.0) .	tText contents: convertedValue asString.! !